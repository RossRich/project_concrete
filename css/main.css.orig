.input-item {
    position: relative;
    height: 72px;
    /*    padding: 16px 0 8px 0;*/
}

.input-item::after {
    background: #FFF;
    transform: scaleX(0);
    transition: transform 0.5s;
}

.input-item::after,
.input-item::before {
    content: '';
    height: 2px;
    width: 250px;
    position: absolute;
    bottom: 6px;
    left: 0;
    background-color: #000;
    /*    transition: height 0.5s;*/
}

.dev-order-input {
    position: relative;
    display: block;
    width: 250px;
    height: 32px;
    padding: 8px 0;
    line-height: 16px;
    font-size: 16px;
    background: transparent;
    border: none;
    -webkit-appearance: none;
    outline: none;
}

.dev-order-label {
    position: relative;
    margin: 0;
    color: lighten(#000, 75);
    line-height: 16px;
    font-size: 16px;
    font-weight: 400;
    display: block;
    transform: translateY(40px);
}

.has-label .dev-order-label {
    transform: translateY(0) scale(1);
    font-size: 12px;
    color: #FFF;
}

.has-label.input-item::after {
    background: #FFF;
    transform: scaleX(1);
}


